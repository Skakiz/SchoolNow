        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	//gs.info('[B:iD] Update principal group');
	var staff = new x_287904_schoolnow.KitchenStaff(current);
	var groupName = 'kitchen_staff_group';
	//var principal = new Principal(current);
	gs.info('[B:iD] Update kitchen staff group, staff:' + staff);
	
	if(staff.exists()) {
		var school = current.school.getRefRecord();

		if(current.operation() == "insert") {	
			gs.info('[B:iD] Update kitchen staff insert : ' + school.getValue(groupName));
			staff.addGroup(school.getValue(groupName));
		}
		else if(current.operation() == "delete") {
			gs.info('[B:iD] Update kitchen staff delete ' + school.getValue(groupName));
			staff.removeGroup(school.getValue(groupName));
		}
	}
	
})(current, previous);]]></script>
